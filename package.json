{
  "name": "hotel-reservation-service",
  "version": "0.0.1",
  "description": "hotel reservation service",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": "14 || 16 || 17 || 18"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rimraf dist *.tsbuildinfo .eslintcache",
    "pretest": "yarn run rebuild",
    "openapi-spec": "node ./dist/openapi-spec",
    "prestart": "yarn run rebuild",
    "start": "node -r source-map-support/register .",
    "prepare": "yarn run build",
  "rebuild": "yarn run clean && yarn run build"
  },
  "repository": {
    "type": "git"
  },
  "author": "pin-wang <pin.wang@hp.com>",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/boot": "^5.0.5",
    "@loopback/core": "^4.0.5",
    "@loopback/rest": "^12.0.5",
    "@loopback/rest-explorer": "^5.0.5",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.21",
    "@loopback/testlab": "^5.0.5",
    "@types/node": "^14.18.33",
    "typescript": "~4.8.4"
  }
}
